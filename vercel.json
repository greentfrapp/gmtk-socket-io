{
  "devCommand": "uvicorn main:socketio_app --host 0.0.0.0 --port 8000",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "api/index.py"
    }
  ]
}